#include<bits/stdc++.h>
using namespace std;
int findDuplicate(vector<int>&arr,int n){
    sort(arr.begin(),arr.end());
    int i;
    for(i=0;i<n-1;i++){
        if(arr[i]==arr[i+1]){
            break;
        }
    }
    return arr[i];
}
int main(){
    int n;
    cin>>n;
    int a;
    vector<int>arr(n);
    for(int i=0;i<n;i++){
        cin>>a;
        arr.push_back(a);
    }
    cout<<findDuplicate(arr,n);
    return 0;
}
